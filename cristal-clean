browser_task_sync('create google doc with calculation results')  # Document automation
launch_any_app('Discord')
execute_in_windsurf_terminal('git status')
"""

    interpreter.auto_run = True

    # Verify functions are available
    print("ðŸ” Verifying automation functions...")
    try:
        test_result = calculate_with_calculator("2+2")
        print(f"âœ… Calculator automation: {test_result['result']}")
    except Exception as e:
        print(f"âŒ Calculator automation error: {e}")

    try:
        print("âœ… Browser automation available")
    except Exception as e:
        print(f"âŒ Browser automation error: {e}")

    # Start clean interface
    print()
    interpreter.chat()

except Exception as e:
    print(f"Error: {e}")
    print("Check your Python environment and API keys.")
